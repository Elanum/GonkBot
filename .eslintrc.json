{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "tsconfig.json"
  },
  "extends": ["airbnb-typescript/base"],
  "plugins": ["@typescript-eslint"],
  "ignorePatterns": ["node_modules", "dist"],
  "rules": {
    "@typescript-eslint/explicit-function-return-type": "error"
  }
}
